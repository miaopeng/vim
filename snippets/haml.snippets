# snippets for HAML 
snippet conf
	- content_for :${1:javascript} do
	  ${2}
snippet cj
	- content_for :javascript do
	  ${1}
snippet cc
	- content_for :stylesheet do
	  ${1}
snippet includejs
	= include_javascripts(:${1:init})
snippet jstag
	= javascript_include_tag "${1}"
snippet includecss
	= include_stylesheets(:${1})
snippet csstag
	= stylesheet_link_tag "${1}"
